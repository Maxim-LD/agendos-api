  "main": "server.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node ./dist/server.js",
    "start:prod": "cross-env NODE_ENV=production node ./dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/server.ts",
    "prod": "cross-env NODE_ENV=production nodemon --exec ts-node ./src/server.ts",
    "dev:debug": "cross-env NODE_ENV=development nodemon --exec \"node --inspect=9229 -r ts-node/register ./src/server.ts\"",
    "migrate:make": "knex migrate:make",
    "migrate:dev": "cross-env NODE_ENV=development knex migrate:latest",
    "migrate:prod": "cross-env NODE_ENV=production knex migrate:latest",
    "migrate:rollback-dev": "cross-env NODE_ENV=development knex migrate:rollback",
    "migrate:rollback-prod": "cross-env NODE_ENV=production knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed:dev": "cross-env NODE_ENV=development knex seed:run",
    "seed:prod": "cross-env NODE_ENV=production knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "",
